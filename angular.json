{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "apps-location-frontend": {
      "root": "apps/apps/location-frontend/",
      "sourceRoot": "apps/apps/location-frontend/src",
      "projectType": "application",
      "prefix": "location-adventure",
      "schematics": {
        "@nrwl/schematics:component": {
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/apps/location-frontend",
            "index": "apps/apps/location-frontend/src/index.html",
            "main": "apps/apps/location-frontend/src/main.ts",
            "polyfills": "apps/apps/location-frontend/src/polyfills.ts",
            "tsConfig": "apps/apps/location-frontend/tsconfig.app.json",
            "assets": [
              "apps/apps/location-frontend/src/favicon.ico",
              "apps/apps/location-frontend/src/assets"
            ],
            "styles": ["apps/apps/location-frontend/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/apps/location-frontend/src/environments/environment.ts",
                  "with": "apps/apps/location-frontend/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "apps-location-frontend:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "apps-location-frontend:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "apps-location-frontend:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/apps/location-frontend/src/test.ts",
            "polyfills": "apps/apps/location-frontend/src/polyfills.ts",
            "tsConfig": "apps/apps/location-frontend/tsconfig.spec.json",
            "karmaConfig": "apps/apps/location-frontend/karma.conf.js",
            "styles": ["apps/apps/location-frontend/src/styles.scss"],
            "scripts": [],
            "assets": [
              "apps/apps/location-frontend/src/favicon.ico",
              "apps/apps/location-frontend/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/apps/location-frontend/tsconfig.app.json",
              "apps/apps/location-frontend/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
